   se maca>=12 entao
      resultado<-1*maca
      Escreval("As" ,maca,  "Maças deram R$" ,resultado)
   senao
      resultado<-1.30*maca
      Escreval("As" ,maca,  "Maças deram R$" ,resultado)
   fimse
